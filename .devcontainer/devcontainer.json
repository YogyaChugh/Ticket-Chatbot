    "name": "Python 3.8.10",
    "image": "mcr.microsoft.com/devcontainers/python:3.8",
    "settings": {
      "python.defaultInterpreterPath": "/usr/local/bin/python"
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "extensions": [
      "ms-python.python"
    ],
    "features": {
      "ghcr.io/devcontainers/features/python:1": {
        "version": "3.8.10"
      }
    }
  }
  